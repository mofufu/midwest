<%- model_class = Usermanual -%>
<div class="page-header">
  <h1><%=t '.title', :default => Usermanual %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:document_name) %></dt>
      <dd><pre class="prettyprint"><%= @usermanual.document_name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:document_type) %></dt>
      <dd><pre class="prettyprint"><%= @usermanual.document_type %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              usermanuals_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_usermanual_path(@usermanual), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              usermanual_path(@usermanual),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

